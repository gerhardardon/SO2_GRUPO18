#!/usr/bin/stap

probe syscall.mmap2 {
    if (execname() != "docker"){
        printf("%d %s mmap %d %s\n",
        pid(), execname(), length, ctime())
    }
}

//total 9.5 Gb
probe syscall.munmap {
    if (execname() != "docker"){
        printf("%d %s munmap %d %s\n",
        pid(), execname(), length, ctime())
    }
}